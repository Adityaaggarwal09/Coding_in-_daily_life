#include<stdio.h>
int main()
{
    int n,i,item;
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    scanf("%d",&item); // item to find in the given array
    binary_sort(a,n,item);
}
int binary_sort(int a[],int n,int item)
{
    int l=0,u=n-1,m;
    while(l<=u) {
    m=(l+u)/2;
    if(item==a[m]) {
    printf("Search successful item found");
        return;
    }
    else if(item>a[m])
    l=m+1;
    else
        u=m-1;
    }
    printf("Search unsuccessful");
}
